<?xml version="1.0" encoding="UTF-8" ?>
<profiles xmlns="http://www.eprosima.com/XMLSchemas/fastRTPS_Profiles">
    <transport_descriptors>
        <transport_descriptor>
            <transport_id>HusarnetTransport</transport_id>
            <type>UDPv6</type>
        </transport_descriptor>
    </transport_descriptors>

    <participant profile_name="CustomHusarnetParticipant" is_default_profile="true">
        <rtps>
            <userTransports>
                <transport_id>HusarnetTransport</transport_id>
            </userTransports>
            <useBuiltinTransports>true</useBuiltinTransports>
            <builtin>
                <initialPeersList>
                <!-- Repeat this part for each husernet peer -->
                    <locator>
                        <udpv6>
                            <!--address>husarnet-IPv6-address</address--> <!-- or <address>husarnet-hostname</address> -->
                            <address>bigfootbot</address>
                        </udpv6>
                    </locator>
                <!-- End repeat -->
                <!-- Repeat this part for each husernet peer -->
                    <locator>
                        <udpv6>
                            <!--address>husarnet-IPv6-address</address--> <!-- or <address>husarnet-hostname</address> -->
                            <address>lenovo_docker</address>
                        </udpv6>
                    </locator>
                <!-- End repeat -->
                <!-- Repeat this part for each husernet peer -->
                    <locator>
                        <udpv6>
                            <!--address>husarnet-IPv6-address</address--> <!-- or <address>husarnet-hostname</address> -->
                            <address>lenovolaptop</address>
                        </udpv6>
                    </locator>
                <!-- End repeat -->
                </initialPeersList>
            </builtin>
        </rtps>
    </participant>
</profiles>